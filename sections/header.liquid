{% comment %}
  menu_alignment options:
  - left
  - left-center
  - center-left
  - center-split
  - center
  - right
{% endcomment %}

{%- liquid
  assign main_menu = linklists[section.settings.main_menu_link_list]

  assign menu_alignment = section.settings.main_menu_alignment
  assign logo_alignment = 'left'
  if menu_alignment == 'center-left' or menu_alignment == 'center-split' or menu_alignment == 'center'
    assign logo_alignment = 'center'
  endif
  if menu_alignment == 'right'
    assign logo_alignment = 'left-right'
  endif

  if main_menu == blank
    assign logo_alignment = 'center'
  endif

  assign mobile_menu_style = 'thumb'

  assign template_name = template | replace: '.', ' ' | truncatewords: 2, '' | handle

  assign overlay_header = false
  if template_name == 'index' and section.settings.sticky_index
    assign overlay_header = true
  endif
  if template_name == 'collection' and collection.image and section.settings.sticky_collection
    assign overlay_header = true
  endif
-%}

{%- render 'slide-nav'
  , section: section,
  main_menu: main_menu,
  mobile_menu_style: mobile_menu_style -%}
{%- if settings.cart_type == 'sticky' -%}
  {%- render 'sticky-cart', mobile_menu_style: mobile_menu_style -%}
{%- endif -%}

{%- style -%}
  .slide-nav,
  .slide-nav button {
    font-size: {{ settings.type_navigation_size }}
    px;
  }

  {% if mobile_menu_style == 'thumb' %}
    body {
      {% comment %}
        Thumb menu button height + padding + 40{% endcomment %}
      padding-bottom: 148px;
    }
  {% endif %}

  {% if mobile_menu_style == 'top' %}
    body.screen-layer-open {
      {% comment %}
        Space for the close modal button{% endcomment %}
      padding-bottom: 40px;
    }
  {% endif %}

  {% if overlay_header %}
    @media only screen and (min-width: 769px) {
      .collection-hero__content .section-header--hero {
        margin-top: 40px;
      }
    }
  {% endif %}
{%- endstyle -%}

<div data-section-id="{{ section.id }}" data-section-type="header-section" {% unless template.name == "index" %}class="header-other-pages"{% endunless %}>
  {%- if section.settings.show_announcement -%}
    {%- if section.settings.show_announcement_home_only -%}
      {%- if template == 'index' -%}
        {%- render 'announcement-bar', section: section -%}
      {%- endif -%}
    {%- else -%}
      {%- render 'announcement-bar', section: section -%}
    {%- endif -%}
  {%- endif -%}
  <div id="header_wrapper" 
    data-header-style="{{ section.settings.header_style }}" class="header-wrapper{% if overlay_header %} header-wrapper--overlay is-light{% endif %}" >

    {% comment %}
      Logo is used as a between-screen loader, so we only
      show the mobile logo on the home page (setting to override)
    {% endcomment %}
    {%- assign mobile_logo_only = true -%}

    <header class="site-header{% if section.settings.logo_hide_mobile %}{% unless template_name == 'index' %} small--hide{% endunless %}{% endif %}" data-overlay="{{ overlay_header }}">
      <div class="page-width">
        <div class="header-layout header-layout--{{ menu_alignment }}{% if mobile_logo_only %} header-layout--mobile-logo-only{% endif %}" data-logo-align="{{ logo_alignment }}">

          {%- if logo_alignment == 'left' or logo_alignment == 'left-right' -%}
            <div class="header-item header-item--logo">
              {%- render 'header-logo-block', section: section -%}
            </div>
          {%- endif -%}

          {%- if logo_alignment == 'left' -%}
            <div
              role="navigation"
              aria-label="Primary"
              class="header-item header-item--navigation{% if menu_alignment == 'left-center' %} text-center{% endif %} small--hide">
                {%- render 'header-desktop-nav', main_menu: main_menu, blocks: section -%}
            </div>
          {%- endif -%}

          {%- if logo_alignment == 'center' -%}
            <div
              class="header-item header-item--left header-item--navigation small--hide"
              role="navigation"
              aria-label="Primary">
              {%- if menu_alignment == 'center' or menu_alignment == 'center-split' -%}
                {%- if section.settings.header_search_enable -%}
                  <div class="site-nav">
                    <a href="{{ routes.search_url }}" class="site-nav__link site-nav__link--icon js-modal-open-search-modal js-no-transition">
                      <svg
                        aria-hidden="true"
                        focusable="false"
                        role="presentation"
                        class="icon icon-search"
                        viewBox="0 0 64 64"><path d="M47.16 28.58A18.58 18.58 0 1 1 28.58 10a18.58 18.58 0 0 1 18.58 18.58zM54 54L41.94 42" /></svg>
                      <span class="icon__fallback-text">{{ 'general.search.title' | t }}</span>
                    </a>
                  </div>
                {%- endif -%}
              {%- endif -%}

              {%- if menu_alignment == 'center-left' -%}
                {%- render 'header-desktop-nav', main_menu: main_menu, blocks: section.blocks -%}
              {%- endif -%}
            </div>

            {%- if menu_alignment == 'center-split' -%}
              {%- render 'header-split-nav', main_menu: main_menu, section: section -%}
            {%- endif -%}

            {%- if menu_alignment != 'center-split' -%}
              <div class="header-item header-item--logo">
                {%- render 'header-logo-block', section: section -%}
              </div>
            {%- endif -%}
          {%- endif -%}

          {%- if logo_alignment == 'left-right' -%}
            <div
              role="navigation"
              aria-label="Primary"
              class="header-item header-item--navigation text-right small--hide">
              {%- render 'header-desktop-nav', main_menu: main_menu, blocks: section -%}
          {%- endif -%}

          {% comment %}
            Center mobile logo if icons are not visible
          {% endcomment %}
          <div class="header-item header-item--icons{% if mobile_logo_only %} small--hide{% endif %}">
            {%- render 'header-icons', section: section, menu_alignment: menu_alignment -%}
            </div>
          </div>
        </div>

        {%- if menu_alignment == 'center' -%}
          <div
            role="navigation"
            aria-label="Primary"
            class="text-center">
            {%- render 'header-desktop-nav', main_menu: main_menu, blocks: section -%}
          </div>
        {%- endif -%}
      </div>
    </header>
  </div>

  {%- if mobile_menu_style == 'thumb' -%}
    {%- if main_menu != blank -%}
      <div class="site-nav__thumb-menu site-nav__thumb-menu--inactive">
        <div class="mobile-menu">
          <button type="button" class="btn site-nav__thumb-button js-toggle-slide-nav">
            <svg
              class="hamburger"
              width="24"
              height="24"
              viewBox="0 0 24 24"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <rect
                x="1"
                y="4"
                width="22"
                height="1.5"
                fill="white" />
              <rect
                x="1"
                y="11"
                width="18"
                height="1.5"
                fill="white" />
              <rect
                x="1"
                y="18"
                width="21"
                height="1.5"
                fill="white" />
            </svg>
            <svg
              aria-hidden="true"
              focusable="false"
              role="presentation"
              class="icon icon-close"
              viewBox="0 0 64 64"><path d="M19 17.61l27.12 27.13m0-27.12L19 44.74" /></svg>
            <span class="icon-menu-label">{{ 'general.drawers.navigation' | t }}</span>
          </button>
          <a href="/collections/clutch-chargers" class="shop-link">Shop</a>
        </div>
        <div class="logo-mobile-center">
          {%- render 'header-logo-block', section: section -%}
        </div>
        <div class="actions-mobile">
          <a class="site-nav__link site-nav__link--icon large--hide" href="{{ routes.account_url }}">
            <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" viewBox="0 0 32 32" fill="none">
              <path d="M26.6667 28V25.3333C26.6667 23.9188 26.1048 22.5623 25.1046 21.5621C24.1044 20.5619 22.7478 20 21.3333 20H10.6667C9.25217 20 7.89562 20.5619 6.89543 21.5621C5.89523 22.5623 5.33333 23.9188 5.33333 25.3333V28" stroke="#F0F0F0" stroke-width="1.33333" stroke-linecap="round" stroke-linejoin="round"/>
              <path d="M16 14.6667C18.9455 14.6667 21.3333 12.2789 21.3333 9.33333C21.3333 6.38781 18.9455 4 16 4C13.0545 4 10.6667 6.38781 10.6667 9.33333C10.6667 12.2789 13.0545 14.6667 16 14.6667Z" stroke="#F0F0F0" stroke-width="1.33333" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
            <span class="icon__fallback-text">
              {%- if customer -%}
                {{ 'layout.customer.account' | t }}
              {%- else -%}
                {{ 'layout.customer.log_in' | t }}
              {%- endif -%}
            </span>
          </a>
          {%- comment -%} <button class="search-trigger">
            <svg
              width="24"
              height="24"
              viewBox="0 0 24 24"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <path d="M21.4433 20.4547L15.996 15.0067C17.091 13.6897 17.7503 11.9992 17.7503 10.1535C17.7503 5.95725 14.349 2.55675 10.1535 2.55675C5.95801 2.55675 2.55676 5.958 2.55676 10.1535C2.55676 14.349 5.95876 17.7502 10.1535 17.7502C12 17.7502 13.6905 17.091 15.0068 15.996L20.454 21.444L21.4433 20.4547ZM3.35626 10.1535C3.35626 6.40575 6.40501 3.35625 10.1535 3.35625C13.9013 3.35625 16.9508 6.40575 16.9508 10.1535C16.9508 13.9012 13.9013 16.9507 10.1535 16.9507C6.40501 16.9507 3.35626 13.9012 3.35626 10.1535Z" fill="white" />
            </svg>
          </button> {%- endcomment -%}
          <button
            href="{{ routes.cart_url }}"
            class="site-nav__thumb-cart openCart"
            aria-controls="CartDrawer"
            data-icon="{{ settings.cart_icon }}">
            <span class="cart-link">
              {%- if settings.cart_icon == 'cart' -%}
                <svg
                  width="25"
                  height="24"
                  viewBox="0 0 21 19"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                  xmlns:xlink="http://www.w3.org/1999/xlink">
                  <rect
                    width="21"
                    height="19"
                    fill="url(#cart-icon-svg)" />
                  <defs>
                    <pattern
                      id="cart-icon-svg"
                      patternContentUnits="objectBoundingBox"
                      width="1"
                      height="1">
                      <use xlink:href="#image0_588_23" transform="translate(0 -0.00239234) scale(0.00378788 0.0041866)" />
                    </pattern>
                    <image
                      id="image0_588_23"
                      width="264"
                      height="240"
                      xlink:href="data:image/png;base64," />
                  </defs>
                </svg>
              {%- else -%}
                <svg
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg">
                  <path
                    fill-rule="evenodd"
                    clip-rule="evenodd"
                    d="M10.7032 3.99513C10.2104 5.02632 9.875 6.80908 9.875 9.75H8.875C8.875 6.76967 9.20902 4.80243 9.8009 3.56394C10.1008 2.93633 10.4765 2.4756 10.9272 2.17377C11.381 1.86981 11.8767 1.75 12.375 1.75C12.8733 1.75 13.369 1.86981 13.8228 2.17377C14.2735 2.4756 14.6492 2.93633 14.9491 3.56394C15.541 4.80243 15.875 6.76967 15.875 9.75H14.875C14.875 6.80908 14.5396 5.02632 14.0468 3.99513C13.8044 3.48789 13.5339 3.18386 13.2663 3.00463C13.0019 2.82754 12.7073 2.75 12.375 2.75C12.0427 2.75 11.7481 2.82754 11.4837 3.00463C11.2161 3.18386 10.9456 3.48789 10.7032 3.99513Z"
                    fill="white" />
                  <path
                    fill-rule="evenodd"
                    clip-rule="evenodd"
                    d="M5.07411 6.25H19.6723L21.1163 21.0238H3.6261L5.07411 6.25ZM5.98089 7.25L4.7289 20.0238H20.0137L18.7652 7.25H5.98089Z"
                    fill="white" />
                </svg>
              {%- endif -%}
              <span class="icon__fallback-text">{{ 'layout.cart.title' | t }}</span>
              <span class="cart-link__bubble{% if cart.item_count > 0 %} cart-link__bubble--visible{% endif %}">
                <span class="cart-link__bubble-num" aria-hidden="true">
                  {{ cart.item_count }}
                </span>
              </span>
            </span>
          </button>
        </div>
        <div class="mobile-search-form">
          <button class="close-form">
            <svg
              width="9"
              height="22"
              viewBox="0 0 9 22"
              fill="none"
              xmlns="http://www.w3.org/2000/svg">
              <path d="M8.49219 16H7.17285L4.47266 11.5977L1.73145 16H0.494141L3.82324 10.791L0.726562 6.00586H2.01855L4.51367 9.99121L7.02246 6.00586H8.25293L5.16309 10.7705L8.49219 16Z" fill="#DEDEDE" />
            </svg>
          </button>
          <form
            action="{{ routes.search_url }}"
            method="get"
            class="input-group search-bar {{ search_bar_location }}"
            role="search">
            <input
              type="hidden"
              name="type"
              value="{{ settings.search_type }}">
            <input
              type="hidden"
              name="options[prefix]"
              value="last">
            <input
              type="search"
              name="q"
              required
              value="{{ search.terms | escape }}"
              placeholder="Start typing..."
              class="input-group-field"
              aria-label="{{ 'general.search.placeholder' | t }}">
            <span class="input-group-btn">
              <button type="submit" class="{{ search_btn_style }}">
                <svg
                  width="24"
                  height="24"
                  viewBox="0 0 24 24"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg">
                  <path d="M21.4433 20.4547L15.996 15.0067C17.091 13.6897 17.7503 11.9992 17.7503 10.1535C17.7503 5.95725 14.349 2.55675 10.1535 2.55675C5.95801 2.55675 2.55676 5.958 2.55676 10.1535C2.55676 14.349 5.95876 17.7502 10.1535 17.7502C12 17.7502 13.6905 17.091 15.0068 15.996L20.454 21.444L21.4433 20.4547ZM3.35626 10.1535C3.35626 6.40575 6.40501 3.35625 10.1535 3.35625C13.9013 3.35625 16.9508 6.40575 16.9508 10.1535C16.9508 13.9012 13.9013 16.9507 10.1535 16.9507C6.40501 16.9507 3.35626 13.9012 3.35626 10.1535Z" fill="white" />
                </svg>
                <span class="icon__fallback-text">{{ 'general.search.submit' | t }}</span>
              </button>
            </span>
          </form>
        </div>
      </div>
    {%- endif -%}
  {%- endif -%}
</div>

{%- assign collections_blocks = section.blocks | where: "type", "collection_mobile" -%}
{%- if collections_blocks.size > 0 and template == "index" -%}
  <div class="collections-mobile v1">
    <div class="scrollable">
      {% for block in collections_blocks %}
        
        <div class="collection-mobile">
          <a href="{{ block.settings.collection }}{% if block.settings.tag != blank %}?tag={{ block.settings.tag }}{% endif %}">
            <img class="lazyload" data-src="{{ block.settings.image | img_url: 'master' }}">
            <h4 class="collection-title">{{ block.settings.title }}</h4>
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
{%- endif -%}

{%- if section.settings.header_search_enable -%}
  {%- render 'search-modal' -%}
{%- endif -%}

<style>
  .collections-mobile {
    overflow-x: scroll;
    max-width: 100vw;
    padding: 20px 10px 18px;
    scrollbar-width: none;
    scroll-behavior: smooth;
    -ms-overflow-style: -ms-autohiding-scrollbar;
  }
  .collections-mobile::-webkit-scrollbar {
    height: 0;
    width: 0;
  }
  .collections-mobile .scrollable {
    width: -webkit-max-content;
    width: -moz-max-content;
    width: max-content;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-column-gap: 14px;
    -moz-column-gap: 14px;
    column-gap: 14px;
  }
  .collection-mobile {
    display: inline-block;
    max-width: 96px;
  }
  .collection-mobile img {
    max-width: 89px;
    display: block;
    margin-bottom: 8px;
  }
  .collection-mobile .collection-title {
    font-size: 16px;
    line-height: 1.375;
    text-align: center;
    letter-spacing: 0.035em;
    text-transform: none;
    margin-bottom: 0;
  }
  @media screen and (min-width: 768px) {
    .collections-mobile {
      display: none !important;
    }
  }
  .collection-mobile:last-child {
    margin-right: 10px;
  }
</style>

