        "type": "header",
        "content": "Announcement bar"
      }, {
        "type": "checkbox",
        "id": "show_announcement",
        "label": "Show an announcement"
      }, {
        "type": "checkbox",
        "id": "show_announcement_home_only",
        "label": "Home page only"
      }, {
        "type": "text",
        "id": "announcement_text",
        "label": "Announcement text",
        "default": "Free shipping and returns"
      }, {
        "type": "url",
        "id": "announcement_link",
        "label": "Announcement link"
      }
    ],
    "blocks": [
      {
        "type": "announcement",
        "name": "Announcement",
        "settings": [
          {
            "type": "text",
            "id": "announcement_text",
            "label": "Text",
            "default": "Announce something here"
          }, {
            "type": "url",
            "id": "announcement_link",
            "label": "Link"
          }
        ]
        }, 
        {
            "type": "collection_mobile",
            "name": "Collection Mobile",
            "settings": [
                {
                    "type": "url",
                    "id": "collection",
                    "label": "Collection"
                }, 
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Image Collection"
                }, 
                {
                    "type": "text",
                    "id": "title",
                    "label": "Title"
                }
            ]
        },
        {
            "type": "dropdown_shop",
            "name": "Dropdown shop item",
            "settings": [
                {
                    "type": "url",
                    "id": "link",
                    "label": "Url item"
                },
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Imagen"
                },
                {
                    "type": "text",
                    "id": "title",
                    "label": "Title"
                }
            ]
        }

    ]
  }
{% endschema %}

<script>
  document.addEventListener("DOMContentLoaded", function() {
    var elem = document.querySelector('.announcement__wrapper');
    var flkty = new Flickity(elem, {
      contain: true,
      autoPlay: 5000,
      prevNextButtons: false,
      pageDots: false,
      on: {
        ready: function() {
          setTimeout(() => {
            document.body.style.paddingTop = `${
              document.querySelector('.announcement').offsetHeight
            }px`;
          }, 5);
        }
      }
    });
    {%- comment -%} document
      .querySelector('.search-trigger')
      .addEventListener('click', event => {
        event
          .target
          .closest('.actions-mobile')
          .nextElementSibling
          .classList
          .add('active')
      }) {%- endcomment -%}
      document
      .querySelector('.close-form')
      .addEventListener('click', event => {
        event
          .target
          .closest('.mobile-search-form')
          .classList
          .remove('active')
      })
    if (document.querySelector('.collections-mobile')) {
      document.querySelector('.main-content').insertBefore(document.querySelector('.collections-mobile'), document.querySelector('.main-content :first-child'));
    }

    window.allowAb = true;

    if (!window.allowAb) return;

    const btnOpenShopDesktop = document.querySelector("#site-nav__openshopDesktop");
    if(!btnOpenShopDesktop) return;

    const header_other_pages = document.querySelector('.header-other-pages');
    const site_header = document.querySelector('.site-header');
    site_header.classList.add('overlay');
    
    /*btnOpenShopDesktop.addEventListener('click', function(e){
      e.preventDefault();
      const sub = document.querySelector('.site-nav__wrapsub');
      const site_header = document.querySelector('.site-header');
      
      site_header.classList.toggle('overlay');
      sub.classList.toggle('open');
    });*/
    
    
  })
