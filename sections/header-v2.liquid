{% schema %}
{
    "name": "Header v2",
    "class": "header-v2-section",
    "settings": [
        {
            "type": "url",
            "id": "highlighted_link",
            "label": "Highlight link"
        },
        {
            "type": "text",
            "id": "highlighted_badge",
            "label": "Highlight badge",
            "placeholder": "New"
        }
    ],
    "blocks": [
        {
            "name": "Hamburger Menu",
            "type": "hamburger_menu",
            "limit": 1,
            "settings": [
                {
                    "label": "Hamburger Icon",
                    "type": "image_picker",
                    "id": "hamburger_icon"
                },
                {
                    "label": "Logo for Floating Menu",
                    "type": "image_picker",
                    "id": "floating_menu_logo"
                },
                {
                    "label": "Menu Selector",
                    "type": "link_list",
                    "id": "link_list",
                    "info": "This menu must only have a first level, otherwise only the parent items will be displayed."
                },
                {
                    "type": "header",
                    "content": "Additional info"
                },
                {
                    "label": "Include Account Link",
                    "type": "checkbox",
                    "id": "account_link"
                },
                {
                    "type": "text",
                    "id": "phone_number",
                    "label": "Contact number",
                    "placeholder": "(561) 576-7578"
                },
                {
                    "type": "text",
                    "id": "mail",
                    "label": "Contact mail",
                    "placeholder": "hello@clutchcharger.com"
                },
                {
                    "label": "Include Social Links",
                    "type": "checkbox",
                    "id": "social_links"
                }
            ]
        },
        {
            "name": "Logo",
            "type": "logo",
            "limit": 1,
            "settings": [
                {
                    "label": "Main Logo",
                    "type": "image_picker",
                    "id": "main_logo"
                }
            ]
        },
        {
            "name": "Mega Menu",
            "type": "mega_nav",
            "limit": 1,
            "settings": [
                {
                    "label": "Menu Selector",
                    "type": "link_list",
                    "id": "link_list",
                    "info": "In order to display the mega menu, the menu item must have children and grandchildren, the children will be displayed as titles of the mega menu and the grandchildren will be the links, in case of not reaching this level, the children will be displayed as a floating menu below the menu item."
                },
                {
                    "label": "Featured Image",
                    "type": "image_picker",
                    "id": "featured_image"
                },
                {
                    "label": "Featured Image Link",
                    "type": "url",
                    "id": "featured_image_link"
                },
                {
                    "type": "inline_richtext",
                    "id": "featured_image_link_text",
                    "label": "Featured Image Link Label"
                }
            ]
        },
        {
            "name": "Utility Menu",
            "type": "utility_nav",
            "limit": 1,
            "settings": [
                {
                    "label": "Include Account Link",
                    "type": "checkbox",
                    "id": "account_link"
                },
                {
                    "label": "Include Search Link",
                    "type": "checkbox",
                    "id": "search_link"
                },
                {
                    "label": "Include Cart Link",
                    "type": "checkbox",
                    "id": "cart_link"
                }
            ]
        },
        {
            "name": "Mobile Menu",
            "type": "mobile_menu",
            "limit": 1,
            "settings": [
                {
                    "label": "Mobile Menu Selector",
                    "type": "link_list",
                    "id": "link_list"
                },
                {
                    "label": "Include Search Form",
                    "type": "checkbox",
                    "id": "search_form"
                }
            ]
        },
        {
            "type": "announcement",
            "name": "Announcement",
            "settings": [
                {
                    "type": "text",
                    "id": "announcement_text",
                    "label": "Text",
                    "default": "Announce something here"
                }, 
                {
                    "type": "url",
                    "id": "announcement_link",
                    "label": "Link"
                }
            ]
        },
        {
            "type": "collection_mobile",
            "name": "Collection Mobile",
            "settings": [
                {
                    "type": "url",
                    "id": "collection",
                    "label": "Collection"
                }, 
                {
                    "type": "image_picker",
                    "id": "image",
                    "label": "Image Collection"
                }, 
                {
                    "type": "text",
                    "id": "title",
                    "label": "Title"
                }
            ]
        }
    ],
    "presets": [
        {
            "name": "Header V2",
            "blocks": [
                {
                "type": "hamburger_menu"
                },
                {
                "type": "logo"
                },
                {
                "type": "mega_nav"
                },
                {
                "type": "utility_nav"
                }
            ]
        }
    ]
}
{% endschema %}