  <script type="text/javascript">
    var checkoutSteps = (callback) => {
    document.addEventListener("page:load", callback);
            document.addEventListener("page:change", callback);
          }
          checkoutSteps(() => { 
        
            console.log('step',Shopify.Checkout.step,'page',Shopify.Checkout.page,'OrderStatus',Shopify.Checkout.OrderStatus);
            let progressBar = 0;
            switch(Shopify.Checkout.step) {
              case "contact_information":
                if (document.querySelector('.step__footer') != null) {
                  let img = document.createElement("img");
                  img.classList.add('guaranted-safe-checkout');
                  img.src = '{{ "Guaranteed.png" | asset_url }}';
    let stepFooter = document.querySelector('.step__footer');
                  stepFooter.insertBefore(img,stepFooter.children[stepFooter.children.length - 1]);
                }
                progressBar = 0;
                break;
              case "shipping_method":
                if (document.querySelector('.step__footer') != null) {
                  let img = document.createElement("img");
                  img.classList.add('guaranted-safe-checkout');
                  img.src = '{{ "Guaranteed.png" | asset_url }}';
    let stepFooter = document.querySelector('.step__footer');
                  stepFooter.insertBefore(img,stepFooter.children[stepFooter.children.length - 1]);
                }
                progressBar = 33.33;
                if (document.querySelector(".content-box__emphasis").innerText.includes('Free')){
                  current_shipping = document.querySelector(".section--shipping-method .radio__label__primary").innerHTML.replace('Free ','')
                  upgrade_text = '<br class="hidden" /><span class="free-upgrade hidden">Free upgrade</span>';
    document.querySelector(".section--shipping-method .radio__label__primary").innerHTML = current_shipping + upgrade_text;
    
                  stroke_price = '<s class="hidden">$8.00</s><br class="hidden" />';
    upgrade_pice = document.querySelector(".content-box__emphasis").innerHTML;
                  document.querySelector('.content-box__emphasis').innerHTML = stroke_price + upgrade_pice;
                }            
                break;
              case "payment_method":
                if (document.querySelector('.step__footer') != null) {
                  let img = document.createElement("img");
                  img.classList.add('guaranted-safe-checkout');
                  img.src = '{{ "Guaranteed.png" | asset_url }}';
    let stepFooter = document.querySelector('.step__footer');
                  stepFooter.insertBefore(img,stepFooter.children[stepFooter.children.length - 1]);
                }
                progressBar = 66.66;
                break;
              case "review_order":
                progressBar = 100;
                break;
              case "processing":
                progressBar = 100;
                break;
              case "stock_problems":
                break;
              default:
            }
            if (progressBar >= 0) {
              if (document.querySelector('.steps .step-1') != null) {
                document.querySelector('.steps .step-1').classList.add('active');
              }
            }
            if (progressBar >= 33.33) {
              if (document.querySelector('.steps .step-2') != null) {
                document.querySelector('.steps .step-2').classList.add('active');
              }
            }
            if (progressBar >= 66.66) {
              if (document.querySelector('.steps .step-3') != null) {
                document.querySelector('.steps .step-3').classList.add('active');
              }
            }
            if (progressBar >= 99.99) {
              if (document.querySelector('.steps .step-4') != null) {
                document.querySelector('.steps .step-4').classList.add('active');
              }
            }
            if (document.querySelector('.progress-steps .progress-bar') != null) {
              document.querySelector('.progress-steps .progress-bar').style.width = progressBar + "%";
              document.querySelector('.progress-steps .progress-bar').setAttribute('aria-valuenow', progressBar);
              document.querySelector('.progress-steps .progress-bar span').innerHTML = progressBar + "% Complete";
            }
                   
          });
          function positioningWidget() {
            let judgemeWidget = document.querySelector('#judge-me-custom-install');
            if(window.innerWidth < 1000){
              document.querySelector('.main').insertBefore(judgemeWidget, document.querySelector('footer'));
            }else{
              document.querySelector('.sidebar').append(judgemeWidget);
            } 
          }
    
          window.addEventListener('resize', positioningWidget);
          window.addEventListener('DOMContentLoaded',positioningWidget)</script>

