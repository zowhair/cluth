<script>
document.querySelector(".view-more-bundles").addEventListener("click", function() {
    var hiddenRow = document.querySelector(".bundle-row.hidden");
    if (hiddenRow) {
        this.style.display = "none";
        hiddenRow.classList.remove("hidden");
    }
});

var handleLink = function() {
    var allConfigOptions = document.querySelectorAll(".bundle-product-right .real-options .bundle-option");
    var variants = {}
    for (var i = 0; i < allConfigOptions.length; i++) {
        var colorSelected = allConfigOptions[i].querySelector(".config-color.selected");
        var chargerTypeselected = allConfigOptions[i].querySelector(".config-charger.selected");
        if (colorSelected && chargerTypeselected) {
            var selectedColor = colorSelected.getAttribute("data-id");
            var selectedChargerType = chargerTypeselected.getAttribute("data-id");

            if (selectedChargerType == "iPhone") {
                if (selectedColor == "Black") {
                    variants["40057856458838"] = variants["40057856458838"] + 1 || 1;
                } else if (selectedColor == "White") {
                    variants["40057856491606"] = variants["40057856491606"] + 1 || 1;
                } else if (selectedColor == "Pink") {
                    variants["40057856524374"] = variants["40057856524374"] + 1 || 1;
                }
            } else {
                if (selectedColor == "Black") {
                    variants["40076864815190"] = variants["40076864815190"] + 1 || 1;
                } else if (selectedColor == "White") {
                    variants["40076864782422"] = variants["40076864782422"] + 1 || 1;
                } else if (selectedColor == "Pink") {
                    variants["40076864847958"] = variants["40076864847958"] + 1 || 1;
                }
            }
        }
    }

    let endArray = [];
    let totalQty = 0;
    let items = [];
    for (const [key, value] of Object.entries(variants)) {
        endArray.push(key + ":" + value);
        totalQty += parseInt(value);
        items.push({
            quantity: value,
            id: key
        })
    }

    const data = {
        items: items
    };

    const xhr = new XMLHttpRequest();
    xhr.open('POST', '/cart/add.js', true);
    xhr.setRequestHeader('Content-Type', 'application/json; charset=UTF-8');
    xhr.send(JSON.stringify(data));

    //console.log("data", data);
    //location.href = `https://clutchllc.myshopify.com/cart/${endArray.join(",")}?channel=buy_button`;
};

var handleConfigSwitch = function(amount) {
    console.log("handleConfigSwitch", amount);
    var insertRight = document.querySelector(".bundle-product-right .real-options");
    if (insertRight) {
        insertRight.innerHTML = "";
        for (var i = 0; i < amount; i++) {
            var cloneTemplate = document.querySelector(".bundle-option.template").cloneNode(true);
            cloneTemplate.classList.remove("template");
            cloneTemplate.querySelector(".bundle-number").innerText = i + 1;
            cloneTemplate.querySelector(".bundle-name span").innerText = i + 1;
            //color clicks

            var allColors = cloneTemplate.querySelectorAll(".config-options .config-color");
            for (var j = 0; j < allColors.length; j++) {
                allColors[j].addEventListener("click", function() {
                    var allConfigColors = this.closest(".bundle-option").querySelectorAll(".config-options .config-color");
                    for (var k = 0; k < allConfigColors.length; k++) {
                        allConfigColors[k].classList.remove("selected");
                    }
                    if (!this.classList.contains("selected")) {
                        this.classList.add("selected");
                        this.closest(".config-type").querySelector(".config-name span").innerText = this.getAttribute("data-id");
                    }
                });
            }

            //charge type clicks
            var allTypes = cloneTemplate.querySelectorAll(".config-options .config-charger");
            for (var j = 0; j < allTypes.length; j++) {
                allTypes[j].addEventListener("click", function() {
                    var allConfigTypes = this.closest(".bundle-option").querySelectorAll(".config-options .config-charger");
                    for (var k = 0; k < allConfigTypes.length; k++) {
                        allConfigTypes[k].classList.remove("selected");
                    }
                    if (!this.classList.contains("selected")) {
                        this.classList.add("selected");
                        this.closest(".config-type").querySelector(".config-name span").innerText = this.getAttribute("data-id");
                    }
                });
            }

            insertRight.appendChild(cloneTemplate);
        }
    }
};

//bundle clicks
var allBundles = document.querySelectorAll(".bundle-selection");
for (var i = 0; i < allBundles.length; i++) {
    allBundles[i].addEventListener("click", function() {
        var removeSelectedBundles = document.querySelectorAll(".bundle-selection");
        for (var j = 0; j < removeSelectedBundles.length; j++) {
            removeSelectedBundles[j].classList.remove("selected");
        }
        if (!this.classList.contains("selected"))
            this.classList.add("selected");

        var currentId = this.getAttribute("data-id");
        if (currentId) {
            var changeNumber = document.querySelector(".bundle-product .bundle-product-name span");
            if (changeNumber) {
                changeNumber.innerText = currentId;
                //pricing
                var changePricingBefore = document.querySelector(".bundle-product .bundle-product-price .before");
                if (!this.querySelector(".bundle-amount .before")) {
                    changePricingBefore.style.display = "none";
                } else {
                    changePricingBefore.style.display = "inline-block";
                    changePricingBefore.innerText = this.querySelector(".bundle-amount .before").innerText;
                }

                var changePricingAfter = document.querySelector(".bundle-product .bundle-product-price .current");
                if (!this.querySelector(".bundle-amount .current")) {
                    changePricingAfter.style.display = "none";
                } else {
                    changePricingAfter.style.display = "inline-block";
                    changePricingAfter.innerText = this.querySelector(".bundle-amount .current").innerText;
                }

                //savings
                var changeSavings = document.querySelector(".bundle-product .bundle-product-price .bundle-savings");
                if (changeSavings) {
                    if (currentId == 1) {
                        changeSavings.style.display = "none";
                    } else {
                        changeSavings.style.display = "block";
                        var savingsElement = this.querySelector(".bundle-savings");
                        if (savingsElement) {
                            changeSavings.innerText = savingsElement.innerText;
                        }
                    }
                }

                //main image
                var mainImageElement = document.querySelector(".bundle-product .main-product-image img");
                if (mainImageElement) {
                    var currentImage = this.querySelector(".img img");
                    mainImageElement.setAttribute("src", currentImage.getAttribute("src"));
                }

                handleConfigSwitch(parseInt(currentId));
            }
        }
    });
}

allBundles[2].click();

//product image changer
var productImages = document.querySelectorAll(".product-images .product-image");
for (var i = 0; i < productImages.length; i++) {
    productImages[i].addEventListener("click", function() {
        var allProductImages = document.querySelectorAll(".product-images .product-image");
        for (var j = 0; j < allProductImages.length; j++) {
            allProductImages[j].classList.remove("selected");
        }
        if (!this.classList.contains("sected")) {
            this.classList.add("selected");
            var mainImageElement = document.querySelector(".bundle-product .main-product-image img");
            if (mainImageElement) {
                mainImageElement.setAttribute("src", this.querySelector("img").getAttribute("src"));
            }
        }

    });
}


var buyCta = document.querySelector(".bundle-buy");
if (buyCta) {
    buyCta.addEventListener("click", function() {
        handleLink();
    })
}

var specTechCta = document.querySelector(".buying-spec.tech-spec");
if (specTechCta) {
    specTechCta.addEventListener("click", function() {
        document.querySelector(".spec-modal").classList.add("tech");
    });
}


var specDevicesCta = document.querySelector(".buying-spec.compatible-spec");
if (specDevicesCta) {
    specDevicesCta.addEventListener("click", function() {
        document.querySelector(".spec-modal").classList.add("compatible");
    });
}


var exitModal = document.querySelector(".spec-modal .spec-modal-container .exit");
if (exitModal) {
    exitModal.addEventListener("click", function() {
        document.querySelector(".spec-modal").classList.remove("tech");
        document.querySelector(".spec-modal").classList.remove("compatible");
    });
}
